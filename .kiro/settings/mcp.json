{
  "mcpServers": {
    "aws-docs": {
      "command": "uvx",
      "args": ["awslabs.aws-documentation-mcp-server@latest"],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": ["aws-docs.search"]
    },
    "cloudformation-validator": {
      "command": "uvx",
      "args": ["cloudformation-validator-mcp-server@latest"],
      "disabled": false,
      "autoApprove": ["validate-template"]
    }
  }
}